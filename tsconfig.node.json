{
  "extends": "@kwai-explore/tsconfig/tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowSyntheticDefaultImports": true,
    "verbatimModuleSyntax": true
  },
  "include": ["vite.config.mts", "server/**/*.ts"]
}
